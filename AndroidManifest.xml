<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.minikod.otoyakit"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name=".MainActivity"
            android:icon="@drawable/ic_launcher"
            android:label="@string/main_activity_label" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ResultActivity"
            android:icon="@drawable/ic_launcher"
            android:label="@string/result_activity_label" >
        </activity>
        <activity
            android:name=".AboutUsActivity"
            android:icon="@drawable/ic_launcher"
            android:label="@string/aboutus_activity_label" >
        </activity>
        <activity
            android:name=".HistoryActivity"
            android:icon="@drawable/ic_launcher"
            android:label="@string/history_activity_label" >
        </activity>
        <activity
            android:name=".HelpActivity"
            android:icon="@drawable/ic_launcher"
            android:label="@string/help_activity_label" >
        </activity>
        <activity
            android:name=".ProfileActivity"
            android:icon="@drawable/ic_launcher"
            android:label="@string/profile_activity_label" >
        </activity>
    </application>

</manifest>